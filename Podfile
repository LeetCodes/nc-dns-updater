source 'https://github.com/CocoaPods/Specs.git'

workspace 'NC DNS Updater'

platform :osx, '10.8'

target :'NC DNS Updater' do
    xcodeproj 'NC DNS Updater.xcodeproj'
    pod 'AFNetworking', '2.0'
    pod 'NWLogging'
    pod 'NOSwitch'
    pod 'XMLReader-PPTV'
end

target :'NC DNS Updater Daemon' do
    xcodeproj 'NC DNS Updater Daemon/NC DNS Updater Daemon.xcodeproj'
    pod 'AFNetworking', '2.0'
    pod 'NWLogging'
    pod 'XMLReader-PPTV'
end
